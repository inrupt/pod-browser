@prefix : <#>.
@prefix vcard: <http://www.w3.org/2006/vcard/ns#>.

<Group/Group_1.ttl#this>
    a vcard:Group;
    vcard:fn "Group 1".

<Group/Group_2.ttl#this>
    a vcard:Group;
    vcard:fn "Group 2".

<index.ttl#this>
    vcard:includesGroup
        <Group/Group_1.ttl#this>,
        <Group/Group_2.ttl#this>.

